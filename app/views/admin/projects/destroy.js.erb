<% if @project %>
  <% if @project.new_record? %>

  	// Fill the right box with first project details
  	$('#div_project_details').html("<%= escape_javascript(render(:partial=>'summary')) %>");

  <% else %>

  	// Fill the right box with first project details
    $('#div_project_details').html("<%= escape_javascript(render(:partial=>'show')) %>");

  <% end %>

  // Render projects List
    $('#div_project_collections').html("<%= escape_javascript(render(:partial=>'index')) %>");

<% else %>

  var noResultsText = "<div id=\"div_admin_empty\" class=\"box text-color-grey p-80 text-align-center\" style=\"height:200px;\"><%= translate("forms.no_results_found") %></div>";
  $('#div_coupon_index').html(noResultsText);
  $('#div_coupon_details').html(noResultsText);

<% end %>
