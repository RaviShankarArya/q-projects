<% if @success %>

  // Fill the right box with first role details
  $('#div_project_members').html("<%= escape_javascript(render(:partial=>'index', :locals=>{:project => @project})) %>");

<% else %>

  var noResultsText = "<div class=\"box text-color-grey p-80 text-align-center\" style=\"height:200px;\"><%= translate("forms.no_results_found") %></div>";
  $('#div_project_members').html(noResultsText);

<% end %>
